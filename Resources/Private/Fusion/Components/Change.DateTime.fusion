prototype(AE.History:Component.Change.DateTime) < prototype(Neos.Fusion:Component) {
    change = null

    renderer = afx`
        <div class="neos-content-diff__block">
            <div class="neos-content-diff__block__base">
                {Date.format(props.change.original, 'd.m.Y H:i:s')}
            </div>
            <div class="neos-content-diff__block__changed">
                {Date.format(props.change.changed, 'd.m.Y H:i:s')}
            </div>
        </div>
    `
}
